		$(function () {

			// Show filters on button click
			$('.btn-show-filters').click(function(){
				$('.filters').slideToggle();
			});

			// Init select2
			$('.js-select').select2();

			// Initial day for sticky
			var initialDay = 'Thursday July,  9<sup>th</sup> 2020';

			// Fixed header Height
			var fixedHeaderHeight = 90;

			// Init Infinite Scroll
			function initInfiniteScroll() {
				// If the next page link is not null
				if($('.pagination__next').attr('href') != null) {
					$('.concerts-feed').infiniteScroll({
						path: '.pagination__next',
						append: '.concerts-items',
						status: '.scroller-status',
						//hideNav: '.pagination', // Already hidden by css
						history: false,
						scrollThreshold: 4000
					});
				}
			}

			// Sticky day function
			function stickyDay() {
				// Top and bottom spacing for sticky day
				var topSpacing = fixedHeaderHeight;
				var bottomSpacing = $(".contact-map-section").height() + $(".agenda").height() + $(".top-footer").height() + $(".footer").height() + 50;
				if($(window).width() <= 991) bottomSpacing += $(".col-promo").height() + 50;
				// Init Sticky
				$(".day-concert").first().sticky({
					topSpacing:topSpacing,
					bottomSpacing:bottomSpacing,
					zIndex:4
				});
				$('.sticky-wrapper').addClass('mb-2');
			}

			// On resize
			$(window).resize(function() {
				// Unstick day
				$(".day-concert").first().unstick();
				// Call Sticky Day function (to update sticky position)
				stickyDay();
			});

			// On scroll
			$(window).scroll(function() {
				/* Creation of an array that will store indexes of parsed days.
				   This array is prefilled with a first index set to 0, ie the sticky day
				   that will be updated and which offset won't change while scrolling.
				*/
				var days = [0];
				// Parsing days (except the first one which is the sticky day that will be updated)
				$('.day-concert:not(:first)').each(function(index){
					// Calculate day offset 
					var dayOffset = $(this).offset().top - $(document).scrollTop() - fixedHeaderHeight - $(this).height();
					// Push index of current day in days array if day offset is equal to 0 or lower
					if(dayOffset<=0) days.push(index);
				});
				// Get the highest value of the days array (ie the index of the current day that must be displayed in sticky day)
				var highestIndex = Object.keys(days).reduce(function(a, b){ return days[a] > days[b] ? a : b });
				// Fetch the text of the day having the highest index
				var html = highestIndex == 0 ? initialDay : $('.day-concert').eq(highestIndex).html();
				// Update the text only if it has not been updated yet
				if($('.sticky-wrapper h2 span').html() != html) {
					$('.sticky-wrapper h2 span').html(html);
				}
			});
			
			// Get page index from sessionStorage
			var pageIndex = sessionStorage.getItem('page-index');
			// Get next page from sessionStorage if the page index is lower than the total
			// number of pages or set it no null so that infinite scroll does not initiate
			var nextPage = pageIndex < 18 ? sessionStorage.getItem('next-page') : null;
			// Get concerts feed from sessionStorage
			var concertsFeed = sessionStorage.getItem('concerts-feed');
			
			// If concerts feed is set
			if(concertsFeed != undefined) {
								// Remove all saved data from sessionStorage
				sessionStorage.clear();
			}

			// Call Sticky Day function (to initiate)
			stickyDay();

			// Call Infinite Scroll init function
			initInfiniteScroll();

			// When a concert link is clicked
			$('.concerts-feed').on('click', '.concert', function() {
				// If infinite scroll is initiated
				if($('.concerts-feed').data('infiniteScroll')) {
					// Get current page index
					var pageIndex = $('.concerts-feed').data('infiniteScroll').pageIndex;
					// Get next page path
					var nextPage = $('.concerts-feed').infiniteScroll('getPath');
					// Save page index and next page to sessionStorage
					sessionStorage.setItem('page-index', pageIndex);
					sessionStorage.setItem('next-page', nextPage);
				}
				// Get concerts feed
				var concertsFeed = $('.concerts-feed').html();
				// Save concerts-feed to sessionStorage
				sessionStorage.setItem('concerts-feed', concertsFeed);
				// Reach location 
				window.location = $(this).attr('href');
				return false;
			});

			// Datetime picker
			$('#datetimepickerfrom').datetimepicker({
				inline: true,
				date: moment('2020-07-09 00:00:00'),
				defaultDate: moment('2020-07-09 00:00:00'),
				useCurrent: false,
				ignoreReadonly: true,
				locale: 'en',
				format: "DD/MM/YYYY"
			});
			$("#datetimepickerfrom").on("change.datetimepicker", function (e) {
				$('#date_from').val(e.date.format("DD/MM/YYYY"));
			});

		});
		</script>
